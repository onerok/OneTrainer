{
  "name": "onetrainer-web-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "gen:api": "python scripts/export_openapi.py && openapi --input src/lib/api/generated/openapi.json --output src/lib/api/generated --client fetch --useOptions --useUnionTypes --exportSchemas false",
    "smoke:playwright": "bash scripts/playwright_smoke.sh"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tsconfig/svelte": "^5.0.4",
    "openapi-typescript-codegen": "^0.30.0",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "typescript": "^5.6.3",
    "vite": "^5.4.12"
  }
}
